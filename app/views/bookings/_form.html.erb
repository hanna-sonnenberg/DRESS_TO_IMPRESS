
<%= simple_form_for([@outfit, @booking ], local: true) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <%= f.input :address %>
  <%= f.date :start_date %>
  <%= f.date :end_date %>
  <%= f.button :submit, class: "btn btn-dark mb-3" %>
<% end %>
