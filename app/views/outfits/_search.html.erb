
<%= form_tag outfits_path, method: :get do %>
<div class="d-flex align-items-center">
<div class="search">
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find an outfit"
  %>
  </div>
  <div class="submit">
  <%= submit_tag "SEARCH", class: "willbutton" %>
  </div>
</div>
<% end %>
