
<%= form_tag outfits_path, method: :get do %>
<div class="d-flex align-items-center">
<div class="search mr-4">
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find a outfit"
  %>
  </div>
  <div class="submit">
  <%= submit_tag "Search", class: "btn btn-primary" %>
  </div>
</div>
<% end %>
